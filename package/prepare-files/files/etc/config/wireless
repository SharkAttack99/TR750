
config wifi-device 'mt7610e'
	option type 'mt7610e'
	option vendor 'ralink'
	option band '5G'
	option auotch '2'
	option bw '2'
	option ht_gi '1'
	option country 'CN'
	option aregion '4'
	option channel '149'

config wifi-iface 'mt7610eiface'
	option device 'mt7610e'
	option ifname 'rai0'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2+ccmp'
	option ssid 'TETON-5G'
	option key 'goodlife'

config wifi-device 'mt7628'
	option type 'mt7628'
	option vendor 'ralink'
	option band '2.4G'
	option channel '0'
	option auotch '2'
	option bw '1'
	option country 'CN'
	option region '1'
	option ht_bsscoexist '0'
	option autoch_skip '12;13'

config wifi-iface 'mt7628iface'
	option device 'mt7628'
	option ifname 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'TETON-2.4G'
	option encryption 'psk2+ccmp'
	option key 'goodlife'

